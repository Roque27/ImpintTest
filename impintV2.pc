      EXEC SQL SELECT to_char(ORD_FECHA_GENERACION,'dd/mm/yyyy'),
                      to_char(fun_habil_anterior(1,ORD_FECHA_GENERACION),'dd/mm/yyyy'),
                      to_char(ORD_FECHA_VENCIMIENTO,'dd/mm/yyyy'),
