             char   *salida,
             char   *tim_codigo,
             char   *bar_code)
{

  /* BF:
   * DF:
   * EF: */

   EXEC SQL BEGIN DECLARE SECTION;
      varchar v_noti[4];
      varchar v_doc_tipo[3];
      varchar v_not_codigo[4];
