   strcpy(v_txt[1].arr,"carta documento o telegrama");
   strcpy(v_txt[2].arr,"reconexin                 ");
   v_txt[0].len=27;
   v_txt[1].len=27;
   v_txt[2].len=27;
   if(strcmp(v_ord_fec.arr,ord_fec)){
      Str2Varchar(ord_fec,v_ord_fec);
	  for(i=0; i<3; i++)
   	  {
      	EXEC SQL SELECT 'Cargo por '||:v_txt[i]||': $'||TO_CHAR(PPR_PRECIO,'9990.00')||' ms IVA'
	    INTO   :v_descripcion[i]
